<table>
  <thead>
    <tr>
      <th>ドメイン</th>
      <th>登録料金</th>
      <th>更新料金</th>
      <th>レジストラ（登録業者）</th>
    </tr>
  </thead>
  <tbody>
    <% @prices.each do |p|%>
    <tr>
      <td><%= p.domain %></td>
      <td><%= p.register_price %></td>
      <td><%= p.update_price %></td>
      <td><%= p.registrar %></td>
    </tr>
    <% end %>
  </tbody>
</table>
<%= paginate @prices %>
